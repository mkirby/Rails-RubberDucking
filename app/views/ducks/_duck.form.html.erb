<% if flash[:errors] %>
    <% flash[:errors].each do |error| %>
        <p><%= error %></p>
    <% end %>
<% end %>

<%= form_for @duck do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <%= f.label :description %><br>
    <%= f.text_area :description %><br>
    <%= f.label :student_id %><br>
    <%= f.collection_select :student_id, Student.all, :id, :name %><br><br>
    <%= f.submit %>
<% end %>